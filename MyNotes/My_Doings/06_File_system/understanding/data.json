            "filename": "index",
            "fileExtension": "js",
            "content": "const express = require('express');\r\nconst { resolve } = require('path');\r\n\r\nconst app = express();\r\nconst port = 3010;\r\n\r\napp.use(express.static('static'));\r\n\r\napp.get('/', (req, res) => {\r\n  res.sendFile(resolve(__dirname, 'pages/index.html'));\r\n});\r\n\r\napp.listen(port, () => {\r\n  console.log(`Example app listening at http://localhost:${port}`);\r\n});\r\n"
        },
